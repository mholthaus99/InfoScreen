{
     "name": "DisplayGadget",
     "version": "1.0.0",
     "description": "An extensible Arduino display framework with pluggable views, input devices, and LCD renderers.",
     "keywords": [
       "arduino",
       "lcd",
       "hd44780",
       "display",
       "input",
       "iot",
       "framework",
       "esp8266"
     ],
     "authors": [
       {
         "name": "Maxwell Holthaus",
         "email": "mholthaus99@gmail.com",
         "url": "https://github.com/mholthaus99/InfoScreen",
         "maintainer": true
       }
     ],
     "repository": {
       "type": "git",
       "url": "https://github.com/mholthaus99/InfoScreen.git"
     },
     "license": "MIT",
     "frameworks": ["arduino"],
     "platforms": ["espressif8266"],
     "build": {
       "srcFilter": [
         "+<*>"
       ]
     },
     "dependencies": [
       {
         "name": "hd44780",
         "owner": "duinowitchery",
         "version": "^1.3.2"
       },
       {
         "name": "IRremoteESP8266",
         "owner": "crankyoldgit",
         "version": "^2.8.6"
       },
       {
         "name": "Adafruit Unified Sensor",
         "owner": "adafruit",
         "version": "^1.1.15"
       },
       {
         "name": "DHT sensor library",
         "owner": "adafruit",
         "version": "^1.4.6"
       },
       {
         "name": "ArduinoJson",
         "owner": "bblanchon",
         "version": "^7.4.2"
       }
     ],
     "export": {
       "include": [
         "src",
         "examples",
         "README.md",
         "library.json"
       ],
       "exclude": [
        
         "src/main.cpp"
       ]
     }
   }
   